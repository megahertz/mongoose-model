{
  "name": "mongoose-model",
  "version": "0.5.0",
  "description": "Define your mongoose models easily with Typescript",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build && npm run test",
    "build": "tsc",
    "lint": "tslint -t verbose index.ts '{lib|test}/**/*.ts'",
    "humile": "humile test/init.ts 'test/**/*.spec.ts'",
    "test": "npm run lint && npm run humile"
  },
  "repository": "megahertz/mongoose-model",
  "files": [
    "dist"
  ],
  "keywords": [
    "mongoose",
    "odm",
    "model",
    "typescript",
    "decorator"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/mongoose-model/issues",
  "homepage": "https://github.com/megahertz/mongoose-model#readme",
  "peerDependencies": {
    "mongoose": ">=4"
  },
  "devDependencies": {
    "@types/mongoose": "*",
    "@types/node": "*",
    "humile": "^0.2.0",
    "mongoose": "*",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^5.0.1",
    "tslint": "*",
    "typescript": "*"
  }
}
