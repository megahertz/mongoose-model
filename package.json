{
  "name": "mongoose-model",
  "version": "0.4.0",
  "description": "Define your mongoose models easily with Typescript",
  "main": "./dist/index.js",
  "scripts": {
    "prepublish": "npm run build && npm run test",
    "build": "tsc",
    "lint": "tslint -t verbose index.ts '{lib|test}/**/*.ts'",
    "mocha": "mocha -r ts-node/register test/init.ts 'test/**/*.spec.ts'",
    "test": "npm run lint && npm run mocha"
  },
  "typings": "./dist/index.d.ts",
  "repository": "megahertz/mongoose-model",
  "files": [
    "dist"
  ],
  "keywords": [
    "mongoose",
    "odm",
    "model",
    "typescript",
    "decorator"
  ],
  "author": "Alexey Prokhorov",
  "license": "MIT",
  "bugs": "https://github.com/megahertz/mongoose-model/issues",
  "homepage": "https://github.com/megahertz/mongoose-model#readme",
  "peerDependencies": {
    "mongoose": ">=4"
  },
  "devDependencies": {
    "@types/chai": "*",
    "@types/mocha": "*",
    "@types/mongoose": "*",
    "@types/node": "*",
    "chai": "*",
    "mocha": "*",
    "mongoose": "*",
    "ts-node": "*",
    "tslint": "*",
    "typescript": "*"
  },
  "dependencies": {
    "reflect-metadata": "*"
  }
}
